{
    "name": "eriksulymosi/eloquent-sqids",
    "description": "On-the-fly sqids for Laravel Eloquent models.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Ali Tavassoli",
            "email": "mtvs.dev@gmail.com"
        },
        {
            "name": "Erik Sulymosi",
            "email": "erik.sulymosi@gmail.com"
        },
    ],
    "require": {
        "illuminate/database": "^10.0",
        "vinkla/sqids": "^0.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.11",
        "pestphp/pest": "^2.19"
    },
    "autoload": {
        "psr-4": {
            "ErikSulymosi\\EloquentSqids\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ErikSulymosi\\EloquentSqids\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
